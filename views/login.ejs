				if('ontouchstart' in document.documentElement) 
					document.write("<script src='/javascripts/jquery.mobile.ace.min.js'>"+"<"+"/script>");
			</script>

			<script src="/javascripts/bootstrap/bootstrap.min.js"></script>
			<script src="/javascripts/custom/global.js"></script>
		</footer>

		<!-- inline scripts related to this page -->
		<script type="text/javascript" src="/javascripts/custom/login.js"> </script>
	</body>

</html>